generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(cuid())
  email      String    @unique
  name       String
  password   String
  photo      String?
  photo_id       String?
  last_login DateTime? @default(now())
  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
}
